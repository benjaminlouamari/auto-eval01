package metho_package;

import java.util.Scanner;

public class Exercice {

	public static void main(String[] args) {
		
		Scanner clavier = new Scanner(System.in);
		
		System.out.println("Entrez un premier entier: ");
		int NUM_1 = clavier.nextInt();
		
		System.out.println("Entrez un deuxième entier: ");
		int NUM_2 = clavier.nextInt();
		
		System.out.println("Entrez un troisième entier: ");
		int NUM_3 = clavier.nextInt();
		
		clavier.close();
		
		int SWAP = NUM_1;
		
		NUM_1 = NUM_2;
		NUM_2 = NUM_3;
		NUM_3 = SWAP;
		
	}

}
